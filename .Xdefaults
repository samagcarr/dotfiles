URxvt*termName: rxvt-256color

#Scrolling 
URxvt*scrollTtyOutput:		False
URxvt*scrollTtyKeypress:	True
URxvt*scrollWithBuffer:		False
URxvt*scrollBar:		False

#Misc
URxvt*loginShell:	true
URxvt*urgentOnBell:	true
URxvt.cursorBlink:	false
URxvt*colorMode:          on
URxvt*dynamicColors:      on
URxvt*highlightSelection: true
URxvt*underlineColor:     rgb:a1/b5/cd
URxvt*cursprColor*:       rgb:aa/aa/aa

#URL Matching
URxvt*perl-lib:			/usr/lib/urxvt/perl/
URxvt*perl-ext-common:		default,matcher
URxvt*urlLauncher:		/usr/bin/firefox
URxvt*matcher.button:		1
URxvt*keysym.C-u:		perl:	matcher
URxvt*matcher.pattern.1:	\\bwww\\.[\\w-]+\\.[\\w./?&@#-]*[\\w/-]
URxvt*cutchars:			"()*,<>[]{}|'

#Fonts
URxvt*font:	xft:MonteCarlo:pixelsize=11
URxvt*boldFont:	xft:MonteCarlo:pixelsize=11

#Colors
*foreground:	white
*background:	black
*color0:	rgb:00/00/00
*color1:	rgb:98/56/5e
*color2:	rgb:66/82/5d
*color3:	rgb:96/91/76
*color4:	rgb:4d/65/85
*color5:	rgb:96/73/95
*color6:	rgb:5f/7f/7b
*color7:	rgb:b3/b3/b3
*color8:	rgb:73/73/73
*color9:	rgb:cf/a3/a9
*color10:	rgb:ca/f7/bb
*color11:	rgb:ff/f8/bc
*color12:	rgb:83/a3/be
*color13:	rgb:bb/a9/cf
*color14:	rgb:96/cc/cc
*color15:	rgb:ff/ff/ff

